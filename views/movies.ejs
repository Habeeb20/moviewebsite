        top: 50%;
        left: 3%;
        width: 25px;
        height: 25px;
        background: rgb(255, 255, 255, .3);
        border-radius: 50%;
        justify-content: center;
        cursor: pointer;
        transition: 1s linear;
        z-index: 9999999;
        opacity: 0;
        

    }

    header section .buttonr {
        left: unset;
        right: 21.5%;
       
    }

    header section .buttonr {
        opacity: 1;
    }

    header section .buttonl {
        opacity: 1;
    }


    

    
  





    





    

    





    



























